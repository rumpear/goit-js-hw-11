  <div class="photo-card">
    <a class="gallery__link" href={{largeImageURL}}>
      <div class="wrapper">
        <img class="gallery__img" src={{webformatURL}} alt={{tags}} loading="lazy" />
      </div>
     </a>
    <div class="info">
      <p class="info-item">
        <b>Likes: {{likes}}</b>
      </p>
      <p class="info-item">
        <b>Views: {{views}}</b>
      </p>
      <p class="info-item">
        <b>Comments: {{comments}}</b>
      </p>
      <p class="info-item">
        <b>Downloads: {{downloads}}</b>
      </p>
    </div>
  </div>

